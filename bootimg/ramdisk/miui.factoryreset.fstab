#device         mount point      fstype        [device2] [length=]

/dev/block/bootdevice/by-name/system       /system_root    ext4    ro,barrier=1                                                    wait,slotselect
/dev/block/bootdevice/by-name/vendor       /vendor         ext4    ro,barrier=1                                                    wait,slotselect
/dev/block/bootdevice/by-name/userdata     /data           ext4    nosuid,nodev,barrier=1,noauto_da_alloc,discard                  wait,check,forceencrypt=footer
